<?xml version="1.0"?> <xsl:stylesheet xmlns:xsl="http://www.w3.org/1999/XSL/Transform" version="1.0"xmlns:redirect="org.apache.xalan.xslt.extensions.Redirect"extension-element-prefixes="redirect"> <xsl:strip-space elements="//I"/><xsl:output method="text" encoding="iso-8859-1"/><xsl:template match="/*">	<xsl:apply-templates select="/simulation/interface/*/*"/></xsl:template> <xsl:template match="/simulation/interface/*/*"> 	<xsl:if test="not(contains(name(.),'AcceptanceCosine'))">		<xsl:variable name="filename">OutS1100.dat.<xsl:value-of select="../../@id"/>.<xsl:value-of select="name(.)"/>.<xsl:value-of select="../@acceptanceCosineIndex"/>.dat</xsl:variable> <!--			<xsl:variable name="filename"><xsl:value-of select="../../../parameters/parameter[@name='outputFilename']"/>.<xsl:value-of select="../../@id"/>.<xsl:value-of select="name(.)"/>.<xsl:value-of select="../@acceptanceCosineIndex"/>.dat</xsl:variable> -->		<redirect:write select="$filename">			<!-- Outputting data, trimming leading white space if any 			It is the same as <xsl:value-of select="."/>, but the leading white space is gone -->		    <xsl:value-of select="concat(substring(translate(.,' &#9;&#10;&#13;',''),1,1),substring-after(.,substring(translate(.,' &#9;&#10;&#13;',''),1,1)))"/>		</redirect:write>	</xsl:if>	</xsl:template>   </xsl:stylesheet> 